{
  "name": "semver-ts",
  "version": "5.7.1",
  "description": "The semantic version parser for Node.js in TypeScript",
  "main": "lib/semver.js",
  "scripts": {
    "build": "node node_modules/typescript/bin/tsc src/*.ts --target es5 --outDir lib",
    "test": "node node_modules/tape/bin/tape test/**/*.js"
  },
  "devDependencies": {
    "@types/node": "^4.9.0",
    "tape": "^4.10.0",
    "typescript": "^3.4.0"
  },
  "license": "ISC",
  "repository": "thachnn/semver-ts",
  "bin": {
    "semver": "./lib/bin.js"
  },
  "files": [
    "lib/",
    "src/",
    "*.bnf"
  ],
  "prettier": {
    "printWidth": 85,
    "trailingComma": "none",
    "semi": false,
    "singleQuote": true
  },
  "tap": {
    "check-coverage": true
  }
}
